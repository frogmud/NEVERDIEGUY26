{
  "name": "neverdieguy26",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "pnpm --filter @ndg/web dev",
    "dev:party": "npx partykit dev",
    "build": "turbo build",
    "build:web": "pnpm --filter @ndg/web build",
    "deploy:party": "npx partykit deploy",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "sim:pantheon": "pnpm --filter @ndg/ai-engine sim:pantheon",
    "sim:eternal": "pnpm --filter @ndg/ai-engine sim:eternal",
    "sim:extract": "pnpm --filter @ndg/ai-engine sim:extract",
    "sim:chatter": "pnpm --filter @ndg/ai-engine sim:chatter",
    "dev:cms": "pnpm --filter @ndg/asset-cms dev",
    "export:wiki": "npx tsx apps/web/scripts/export-wiki-data.ts"
  },
  "devDependencies": {
    "@vercel/node": "^5.5.23",
    "partykit": "^0.0.111",
    "tsx": "^4.19.0",
    "turbo": "^2.7.5",
    "typescript": "~5.8.3"
  },
  "pnpm": {
    "overrides": {
      "undici": ">=5.29.0",
      "path-to-regexp": ">=6.3.0",
      "tar": ">=7.5.4",
      "diff": ">=7.0.0",
      "esbuild": ">=0.25.0"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.0"
}
